MODULE bee1002;

IMPORT Input, Texts;

VAR
    raio, pi, area: REAL;

PROCEDURE AreaDoCirculo;
BEGIN
    pi := 3.14159;

    Input.Real(raio);

    area := pi * (raio * raio);

    Texts.WriteString(Texts.Console, "A=");
    Texts.WriteReal(Texts.Console, area, 4);
    Texts.WriteLn(Texts.Console);
END AreaDoCirculo;

BEGIN
    AreaDoCirculo;
END bee1002.