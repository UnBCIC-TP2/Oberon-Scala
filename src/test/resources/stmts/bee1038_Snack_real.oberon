MODULE bee1038real;

TYPE
    realArray = ARRAY 5 OF REAL

VAR
    banknotesValues: realArray;
    value: REAL;
    x, y: INTEGER;

BEGIN
    x := 1;
    y := 3;

    banknotesValues[0] := 2.5;
    banknotesValues[1] := 3.5;
    banknotesValues[2] := 4.5;
    banknotesValues[3] := 5.5;
    banknotesValues[4] := 6.5;

    value := banknotesValues[x] * y

END

END bee1038real.