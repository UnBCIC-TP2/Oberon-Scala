MODULE bee1005;

IMPORT Input, Texts;

VAR
    A, B, media: REAL;

PROCEDURE Media;
BEGIN
    Input.Int(A);
    Input.Int(B);

    media := ((A * 3.5) + (B * 7.5)) / 11;

    Texts.WriteString(Texts.Console, "MEDIA = ");
    Texts.WriteReal(Texts.Console, media, 5);
    Texts.WriteLn(Texts.Console);
END Media;

BEGIN
    Media;
END bee1005.