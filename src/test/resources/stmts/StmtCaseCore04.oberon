MODULE SimpleRangeCaseModule;

VAR
  xs : INTEGER;
  min : INTEGER;
  max : INTEGER;

BEGIN
  xs := 12;
  min := 10;
  max := 20;

  CASE xs OF
      1:          xs := 5
    | 2:          xs := 10
    | min .. max: xs := 20
    ELSE          xs := 0
  END;

  write(xs)

END

END SimpleRangeCaseModule.