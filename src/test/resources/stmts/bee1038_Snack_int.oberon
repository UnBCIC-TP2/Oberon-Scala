MODULE bee1038int;

TYPE
    intArray = ARRAY 5 OF INTEGER

VAR
    banknotesValues: intArray;
    value: INTEGER;
    x, y: INTEGER;

BEGIN
    x := 1;
    y := 3;

    banknotesValues[0] := 2;
    banknotesValues[1] := 3;
    banknotesValues[2] := 4;
    banknotesValues[3] := 5;
    banknotesValues[4] := 6;

    value := banknotesValues[x] * y

END

END bee1038int.